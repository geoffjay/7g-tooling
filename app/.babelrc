{
    "presets": ["@babel/env", "@babel/react"],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": false }],
        [
            "module-resolver",
            {
                "alias": {
                    "7g-utils": "/src/renderer/utils",
                    "7g-components": "/src/renderer/components",
                    "7g-pages": "/src/renderer/pages"
                }
            }
        ]
    ]
}
